def isPrime(number) = {
  def check(i) = {
    if i >= number {
      true;
    } else {
      if number % i == 0 {
        false;
      } else {
        check(i + 1);
      }
    }
  }
  check(2);
}

def primeNumbers = [];
def candidate = 2;

while length(primeNumbers) < 100 {
  if isPrime(candidate) {
    push(candidate, primeNumbers);
  } else {
    false;
  }
  candidate = candidate + 1;
}

primeNumbers;